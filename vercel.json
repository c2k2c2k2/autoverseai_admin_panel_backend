{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.ts"
        }
    ],
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "framework": null,
    "regions": [
        "iad1"
    ]
}